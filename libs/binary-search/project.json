{
  "name": "binary-search",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/binary-search/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/binary-search",
        "main": "libs/binary-search/src/index.ts",
        "tsConfig": "libs/binary-search/tsconfig.lib.json",
        "assets": ["libs/binary-search/*.md"],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/binary-search/**/*.ts",
          "libs/binary-search/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/binary-search/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
